{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "react/jsx-filename-extension": 0,
    "jsx-a11y/label-has-for": {
      "required": {
        "some": [ "nesting", "id" ]
      }
    }
  }
}
